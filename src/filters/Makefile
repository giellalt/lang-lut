gttags.txt: ../morphology/$(GTLANG).lexc.hfst
	$(AM_V_GEN)$(HFST_SUMMARIZE) -v $^ \
		| grep -A1 '^sigma set' \
		| grep -F '+' \
		| tr ' ' '\n' \
		| sed 's/,$$//g' \
		| grep -E '(^\+.|.\+$$)' \
		> $@

